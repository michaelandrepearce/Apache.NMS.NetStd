<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <RootNamespace>Apache.NMS</RootNamespace>
    <Product>Apache NMS Class Library for .net standard</Product>
    <Description>Apache NMS (.Net Standard Messaging Library): An abstract interface to Message Oriented Middleware (MOM) providers</Description>
    <Copyright>Copyright (C) 2005-2015 Apache Software Foundation</Copyright>
    <AssemblyVersion>1.7.1.3898</AssemblyVersion>
    <Version>1.7.1.3898</Version>
    <Authors>Apache Software Foundation, William D Cossey</Authors>
    <Company></Company>
    <PackageReleaseNotes>Apache.NMS (http://activemq.apache.org/nms/) for .NET Standard.
Fix for hardcoded assembly names.</PackageReleaseNotes>
    <PackageTags>Apache,NMS,Messaging,NetStandard</PackageTags>
    <PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageLicenseUrl>http://www.apache.org/licenses/LICENSE-2.0.txt</PackageLicenseUrl>
    <RepositoryUrl>https://github.com/wdcossey/Apache.NMS.NetStd</RepositoryUrl>
    <PackageProjectUrl>https://github.com/wdcossey/Apache.NMS.NetStd</PackageProjectUrl>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
    <DefineConstants>TRACE;DEBUG;NETCORE</DefineConstants>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Release|AnyCPU'">
    <DefineConstants>TRACE;NETCORE</DefineConstants>
  </PropertyGroup>

</Project>
